def format(f_name,l_name):
    if f_name == "" or l_name == "":
        return 
    formated_l_name = l_name.title()
    formated_f_name = f_name.title()
    return f"{formated_f_name}  {formated_l_name}"
             

    
print(format(input("what's your first name"),input("what's your second name")))
